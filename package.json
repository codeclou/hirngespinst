{
  "name": "hirngespinst",
  "description": "Create svg infographics with ease",
  "version": "0.0.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeclou/hirngespinst.git"
  },
  "scripts": {
    "start": "./node_modules/.bin/ws -d ./ --port 7676",
    "build": "npm run build:pre && npm run build:cp && npm run build:js && npm run build:css",
    "build:pre": "rm -f dist/*",
    "build:cp": "cp src/* dist/",
    "build:js": "uglifyjs  --compress --mangle --reserved 'Hirngespinst,hirngespinstGlobalState' --comments --output dist/showWithDelay.min.js  src/showWithDelay.js",
    "build:css": "cssnano src/showWithDelay.css dist/showWithDelay.min.css"
  },
  "dependencies": {},
  "devDependencies": {
    "cssnano-cli": "^1.0.5",
    "local-web-server": "^1.2.6",
    "uglify-js": "^2.8.7"
  }
}
